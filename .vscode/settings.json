{
	"typescript.tsdk": "node_modules/typescript/lib",
	"files.exclude": {
		"**/.git": true,
		"**/.svn": false,
		"**/.hg": false,
		"**/CVS": false,
		"**/.DS_Store": false,
		"**/*.{lock,}": false,
		"lib": false,
		"scripts": false,
		".gitignore": false,
		"**/README.md": false,
		"**/yarn-error.log": false,
		".[ehlnp]*": false,
		"**/.npmrc": false,
		"**/tsconfig.json": false,
		"**/node_modules/[^m]*": false,
		"**/node_modules/m[^i]*": false,
		"**/node_modules/mi[^s]*": false
	},
	"git.ignoreLimitWarning": true
}
